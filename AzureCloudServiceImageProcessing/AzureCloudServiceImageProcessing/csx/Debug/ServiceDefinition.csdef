<?xml version="1.0" encoding="utf-8"?>
<ServiceDefinition xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" name="AzureCloudServiceImageProcessing" schemaVersion="2015-04.2.6" xmlns="http://schemas.microsoft.com/ServiceHosting/2008/10/ServiceDefinition">
  <WebRole name="ImageReSizeWebRole">
    <LocalResources>
      <LocalStorage name="DiagnosticStore" sizeInMB="4096" cleanOnRoleRecycle="false" />
    </LocalResources>
    <ConfigurationSettings>
      <Setting name="Images20By20" />
      <Setting name="Images40by40" />
      <Setting name="LoginTable" />
      <Setting name="OriginalImages" />
      <Setting name="QueueName" />
      <Setting name="StorageConnectionString" />
    </ConfigurationSettings>
    <Endpoints>
      <InputEndpoint name="Endpoint1" protocol="http" port="80" localPort="80" />
    </Endpoints>
    <Certificates />
  </WebRole>
  <WorkerRole name="ImageReSizeWorkerRole">
    <LocalResources>
      <LocalStorage name="DiagnosticStore" sizeInMB="4096" cleanOnRoleRecycle="false" />
    </LocalResources>
    <ConfigurationSettings>
      <Setting name="Images20by20" />
      <Setting name="Images40by40" />
      <Setting name="OriginalImages" />
      <Setting name="QueueName" />
      <Setting name="StorageConnectionString" />
      <Setting name="WorkerRoleLogTableName" />
    </ConfigurationSettings>
    <Endpoints />
    <Certificates />
  </WorkerRole>
</ServiceDefinition>